    icon_on: mdi-numeric-8-box
    icon_off: mdi-numeric-8-box
    entity: switch.sky_button_tv_channel_8
nove:
    widget_type: scene
    icon_on: mdi-numeric-9-box
    icon_off: mdi-numeric-9-box
    entity: switch.sky_button_tv_channel_9
su:
    widget_type: scene
    icon_on: mdi-arrow-up-bold
    icon_off: mdi-arrow-up-bold
    entity: switch.sky_button_arrow_up
giu:
    widget_type: scene
    icon_on: mdi-arrow-down-bold
    icon_off: mdi-arrow-down-bold
    entity: switch.sky_button_arrow_down
sinistra:
    widget_type: scene
    icon_on: mdi-arrow-left-bold
    icon_off: mdi-arrow-left-bold
