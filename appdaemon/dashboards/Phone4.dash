    icon_on: mdi-checkbox-marked
    icon_off: mdi-checkbox-marked
    entity: switch.sky_button_ok
esc:
    widget_type: scene
    title: esc
    icon_on: mdi-close-box
    icon_off: mdi-close-box
    entity: switch.sky_button_esc
registra:
    widget_type: scene
    title: Rec
    icon_on: mdi-record-rec
    icon_off: mdi-record-rec
    entity: switch.sky_button_rec
info:
    widget_type: scene
    title: Info
    icon_on: mdi-information
    icon_off: mdi-information
    entity: switch.sky_button_i
giallo:
    widget_type: scene
    title: Giallo
    icon_on: mdi-record
    icon_off: mdi-record
    entity: switch.sky_button_yellow
blu:
    widget_type: scene
    title: Blu
    icon_on: mdi-record
    icon_off: mdi-record
    entity: switch.sky_button_blue
demand-my:
    widget_type: scene
    title: MySky
    icon_on: mdi-play-circle
    icon_off: mdi-play-circle
    entity: switch.sky_button_on_demand