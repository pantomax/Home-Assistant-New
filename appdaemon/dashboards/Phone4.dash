title: Phone4
widget_dimensions: [105, 105]
widget_margins: [10, 8]
columns: 3

layout:
    - info, giallo, blu
    - uno, due, tre
    - quattro, cinque, sei
    - sette, otto, nove
    - esc, zero, ok
    - registra, su, demand-my
    - sinistra, giu, destra
    - include: bottom_panel_phone

zero:
    widget_type: scene
    icon_on: mdi-numeric-0-box
    icon_off: mdi-numeric-0-box
    entity: switch.sky_button_tv_channel_0
uno:
    widget_type: scene
    icon_on: mdi-numeric-1-box
    icon_off: mdi-numeric-1-box
    entity: switch.sky_button_tv_channel_1
due:
    widget_type: scene
    icon_on: mdi-numeric-2-box
    icon_off: mdi-numeric-2-box
    entity: switch.sky_button_tv_channel_2
tre:
    widget_type: scene
    icon_on: mdi-numeric-3-box
    icon_off: mdi-numeric-3-box
    entity: switch.sky_button_tv_channel_3
quattro:
    widget_type: scene
    icon_on: mdi-numeric-4-box
    icon_off: mdi-numeric-4-box
    entity: switch.sky_button_tv_channel_4
cinque:
    widget_type: scene
    icon_on: mdi-numeric-5-box
    icon_off: mdi-numeric-5-box
    entity: switch.sky_button_tv_channel_5
sei:
    widget_type: scene
    icon_on: mdi-numeric-6-box
    icon_off: mdi-numeric-6-box
    entity: switch.sky_button_tv_channel_6
